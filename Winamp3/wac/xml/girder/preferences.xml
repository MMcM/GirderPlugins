<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<!-- Layout of the Girder component Preferences group. -->

<WinampAbstractionLayer version="0.7">

  <groupdef id="Girder.Attribs.Content">

    <Wasabi:CheckBox id="eventsAttrib" x="0" y="0" text="Send events to Girder" 
                     cfgattrib="{57FF2C53-08D7-444a-B20E-CDC520088B33};EventAttrib"/>

    <Wasabi:Text id="intervalAttrib_label" text="Interval (in milliseconds)" 
                 x="0" y="20" w="0" h="16" relatw="1" />
    <Wasabi:EditBox id="wasabi.edit" x="12" y="40" w="-24" h="20" relatw="1"  
                    cfgattrib="{57FF2C53-08D7-444a-B20E-CDC520088B33};IntervalAttrib" />

  </groupdef>

  <groupdef id="Girder">

    <Wasabi:TitleBox x="0" y="0" w="0" h="0" relatw="1" relath="1" 
                     title="Girder" content="Girder.Attribs.Content" />

  </groupdef>

</WinampAbstractionLayer>
