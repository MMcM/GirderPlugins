      <Command Name="Functions" Identifier="140" Enabled="TRUE">
        <Comments>Define LUA functions</Comments>
        <StateCount>1</StateCount>
        <StateBegin>1</StateBegin>
        <StateCurrent>1</StateCurrent>
        <StateInvert>FALSE</StateInvert>
        <OSD>FALSE</OSD>
