        <ActionSubType>34</ActionSubType>
        <sValue1>-- Interface to Windows Management Instrumentation.
-- WMI itself is present in 2K, but the formatted data ("cooked") classes are new to XP.

-- Add a WMI object to LUA variable mapping.
-- name: A unique name for the object; a prefix for the variable names.
-- path: The local WMI path to the object.  Use the WMI Object Browser if unsure of this.
-- properties: A table mapping variable suffix to object property / formatting function.
function WMIAddObject (name, path, properties)
  if not luacom then
    LoadLuaCom()
  end

  if not WMI then
    WMI = {}

    -- Could also do this with monikers, but this is more obvious.
    WMI.locator = luacom.CreateObject("WbemScripting.SWbemLocator")
    WMI.services = WMI.locator:ConnectServer()

    WMI.refresher = luacom.CreateObject("WbemScripting.SWbemRefresher")
    WMI.refresher.AutoReconnect = 1

    WMI.objects = {}
  end

  local object, entry
  object = WMI.refresher:Add(WMI.services, path).Object
  entry = { name = name, path = path, object = object, properties = properties }
  WMI.objects[name] = entry
end

-- Get rid of the global object that holds all our COM references.
function WMIClearObjects ()
  WMI = nil
  collectgarbage()
end

-- Refresh all previously added objects, setting LUA variables.
function WMIRefreshObjects ()
  WMI.refresher:Refresh()
  for name, entry in WMI.objects do
    WMIRefreshOne(entry.name, entry.object, entry.properties)
  end
end

-- Refresh this object according to its set properties.
function WMIRefreshOne(prefix, object, properties)
  local var, val, fun
  -- It works to just have a property name in the table, in which case
  -- an ordinal name is generated.
  for key, prop in properties do
    var = prefix .. key
    -- If the first character of the property is a colon, then it's
    -- the name of a function defined in here, rather than a native
    -- property.  E.g. ":WMIFormatDiskFreeSpace".
    if (strbyte(prop) == 58) then
      fun = getglobal(strsub(prop, 2))
    else
      fun = object[prop]
    end
    val = fun(object)
    setglobal(var, val)
  end
end

-- Format logical disk free space, showing name, bytes and percentage.
function WMIFormatDiskFreeSpace(object)
  local freemb, freeb, freepct
  freemb = object:FreeMegabytes()
  freepct = object:PercentFreeSpace()
  if not freemb then return nil end
  if freemb &gt; 512 then
    -- Follow the Explorer convention of 1GB = 1024MB.  Even if you
    -- think 1GB = 1000MB, this is close enough.
    freeb = format("%.1fGB", freemb / 1024)
  else
    freeb = format("%fMB", freemb)
  end
  return object:Name() .. " " .. freeb .. " free (" .. freepct .. "%)"
end

