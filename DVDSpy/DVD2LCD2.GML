        <sValue1>require 'Classes.WMI'

-- Get number of active server sessions.
function Classes.WMI.GetServerSessions(self)
  if not self.refobjServer then
    self.refobjServer = self.oRefresher:AddEnum(self.oWMIService,"Win32_PerfFormattedData_PerfNet_Server").ObjectSet 
  end
 local result
 self.oRefresher:Refresh() 
 for index,item in luacomE.pairs(self.refobjServer) do 
   result = item:ServerSessions() 
 end 
 return result or 0
end

-- Get name and percent for the most active process.
function Classes.WMI.MostActiveProcess(self, ignoreIdle)
  if not self.refobjProcesses then
    self.refobjProcesses = self.oRefresher:AddEnum(self.oWMIService,"Win32_PerfFormattedData_PerfProc_Process").ObjectSet
  end
  self.oRefresher:Refresh()
  local pct, pname, maxpct, maxname
  for index,proc in luacomE.pairs(self.refobjProcesses) do
    pct = proc:PercentProcessorTime()
    if (not maxpct) or (pct &gt; maxpct) then
      pname = proc:Name()
      if (pname ~= "_Total") and (includeIdle or (pname ~= "Idle")) then
        maxpct, maxname = pct, pname
      end
    end
  end
  if maxpct then
    return maxname, maxpct
  end
end

Classes.WMIFormatted = Classes.WMI:New()

-- Format logical disk free space, showing name, bytes and percentage.
function Classes.WMIFormatted.FormatDiskFreeSpace(self)
  self.oRefresher:Refresh () 
  local result
  for index,item in luacomE.pairs(self.refobjDisk) do 
    local freemb, freeb, freepct
    freemb = item:FreeMegabytes()
    freepct = item:PercentFreeSpace()
    if freemb then
      if freemb &gt; 512 then
        -- Follow the Explorer convention of 1GB = 1024MB.  Even if you
        -- think 1GB = 1000MB, this is close enough.
        freeb = string.format("%.1fGB", freemb / 1024)
      else
        freeb = string.format("%fMB", freemb)
      end
	  if result then result = result .. "\n" else result = "" end
	  result = result .. item:Name() .. " " .. freeb .. " free (" .. freepct .. "%)"
    end
  end
  return result
end

function Classes.WMIFormatted.FormatMostActiveProcess(self, ignoreIdle)
  local name, pct = self:MostActiveProcess(ignoreIdle)
  if name then
    return name .. " (" .. pct .. "%)"
  end
end

