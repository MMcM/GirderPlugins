      <Comments>Weather info displayed when idle *OFF*</Comments>
      <Command Name="Init" Identifier="137" Enabled="TRUE" ErrorBreak="FALSE" ErrorJump="FALSE" ErrorEmail="FALSE" ErrorGirderLog="FALSE" ErrorSystemLog="FALSE">
        <OSDTimeout>1970239776</OSDTimeout>
        <OSD>TRUE</OSD>
        <Topmost>TRUE</Topmost>
        <MatchNum>1</MatchNum>
        <sValue1>function WeatherConditionsUpdated(eventstring, eventdevice)
  local cc = weather.Local.cc
  if not cc then return end
  WeatherConditions = math.round(cc.Temperature) .. "F " .. cc.Humidity .. "%"
  WeatherWind = cc.WindDirectionShort .. math.round(cc.WindSpeed)
  if weather.Local.fc then
    local fc = weather.Local.fc[1]
    WeatherHiLo = math.round(fc.HighTemp) .. "/" .. math.round(fc.LowTemp)
    WeatherForecast = fc.Description
  end
  gir.TriggerEvent("Weather Variables Updated", eventdevice)
end

local handler = gir.AddEventHandler("Conditions updated", 10015, 10015, WeatherConditionsUpdated)
gir.AddScriptResetCallback(function() gir.RemoveEventHandler(handler) end)
