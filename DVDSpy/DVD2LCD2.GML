-- Interface to Windows Management Instrumentation.
-- WMI itself is present in 2K, but the formatted data ("cooked") classes are new to XP.

-- Ensure that we have a connection to the WMI server.
-- Mostly internal.
function WMIOpen()
  if not luacom then
    LoadLuaCom()
  end

  if not WMI then
    WMI = {}

    -- Could also do this with monikers, but this is more obvious.
    WMI.locator = luacom.CreateObject("WbemScripting.SWbemLocator")
    WMI.services = WMI.locator:ConnectServer()

    WMI.refresher = luacom.CreateObject("WbemScripting.SWbemRefresher")
    WMI.refresher.AutoReconnect = 1

    WMI.objects = {}
  end
end

-- Get rid of the global object that holds all our COM references.
function WMIClose()
  WMI = nil
  collectgarbage()
end

-- Add a WMI object to LUA variable mapping.
-- name: A unique name for the object; a prefix for the variable names.
-- path: The local WMI path to the object.  Use the WMI Object Browser if unsure of this.
-- handler: See WMIAddObjectEntry.
function WMIAddPath(name, path, handler)
  WMIOpen()
  local object = WMI.refresher:Add(WMI.services, path).Object
  WMIAddObjectEntry(name, object, handler)
end

-- Add WMI objects satisfying a query.
-- name: A unique name for the object; a prefix for the variable names.
-- query: A WQL query.
-- handler: See WMIAddObjectEntry.
-- max: The maximum number of objects to add; default unbounded.
function WMIAddQuery(name, query, handler, max)
  WMIOpen()
  local rs, iter, obj, idx, suff
  rs = WMI.services:ExecQuery(query)
  iter = luacom.GetEnumerator(rs)
  idx = 1
  suff = ""
  if max ~= idx then suff = idx end
  while (not max) or (idx &lt;= max) do
    obj = iter:Next()
    if not obj then break end
    WMIAddPath(name .. suff, obj:Path_(), handler)
    idx = idx + 1
    suff = idx
  end
end

-- Add a set of WMI objects.
-- Note: WMIAddQuery adds a set of objects present at the time of the
-- add.  This function refreshes the set itself each time.  It is
-- therefore useful for things like active processes.
-- name: A unique name for the object; a prefix for the variable names.
-- class: The object class name.
-- handler: A function.
function WMIAddEnum(name, class, handler)
  WMIOpen()
  local enum = WMI.refresher:AddEnum(WMI.services, class).ObjectSet
  WMIAddObjectEntry(name, enum, handler)
end

-- Add a refreshing object to the table.
-- name: A unique name for the object; used to form global variable names.
-- object: The COM object.
-- handler: A handler function, called with name and object.
--  or a string property name, setting name to the value of that property.
--  or a table (see WMIPropertiesHandler)
function WMIAddObjectEntry(name, object, handler)
  local ht = type(handler)
  local hfun, harg
  if ht == "function" then
    hfun = handler
  else
    harg = handler
    if ht == "string" then
      hfun = WMISinglePropertyHandler
    else
      hfun = WMIPropertiesHandler
    end
  end
  local entry = { object = object, handler = hfun, arg = harg }
  WMI.objects[name] = entry
end

-- Refresh all previously added objects, setting LUA variables.
function WMIRefresh()
  if not WMI then return end
  WMI.refresher:Refresh()
  for name, entry in WMI.objects do
    entry.handler(name, entry.object, entry.arg)
  end
end

-- Set a variable to the value of a single property.
function WMISinglePropertyHandler(var, object, property)
  setglobal(var, object[property](object))
end

-- Set variables according to a table of properties.
-- prefix: The prefix for variable names.
-- object: The WMI object.
-- properties: A table; each key is the variable suffix (including an ordinal when
-- just value is given to the table constructor); each value is a handler function
-- called with the object to return a formatted value or a string property name.
function WMIPropertiesHandler(prefix, object, properties)
  local var, val, fun
  -- It works to just have a property name in the table, in which case
  -- an ordinal name is generated.
  for key, prop in properties do
    var = prefix .. key
    if (type(prop) == "function") then
      fun = prop
    else
      fun = object[prop]
    end
    val = fun(object)
    setglobal(var, val)
  end
end

-- Format logical disk free space, showing name, bytes and percentage.
function WMIFormatDiskFreeSpace(object)
  local freemb, freeb, freepct
  freemb = object:FreeMegabytes()
  freepct = object:PercentFreeSpace()
  if not freemb then return nil end
  if freemb &gt; 512 then
    -- Follow the Explorer convention of 1GB = 1024MB.  Even if you
    -- think 1GB = 1000MB, this is close enough.
    freeb = format("%.1fGB", freemb / 1024)
  else
    freeb = format("%fMB", freemb)
  end
  return object:Name() .. " " .. freeb .. " free (" .. freepct .. "%)"
end

-- Get name and percent for the most active process
function WMIMostActiveProcess(name, enum, ignore1, includeIdle)
  local iter, proc, pct, pname, maxpct, maxname
  iter = luacom.GetEnumerator(enum)
  proc = iter:Next()
  while proc do
    pct = proc:PercentProcessorTime()
    if (not maxpct) or (pct &gt; maxpct) then
      pname = proc:Name()
      if (pname ~= "_Total") and (includeIdle or (pname ~= "Idle")) then
        maxpct, maxname = pct, pname
      end
    end
    proc = iter:Next()
  end
  if maxpct then
    setglobal(name, maxname .. " (" .. maxpct .. "%)")
  end
end

-- Append tables.
function tappend(...)
  t = {}
  local n = 0
  for i = 1, getn(arg) do
    local at = arg[i]
    if at then
      for j = 1, getn(at) do
        n = n + 1
        t[n] = at[j]
      end
    end
  end
  t.n = n
  return t
end

-- Curry function with additional args on the right.
function rcurry(f, ...)
  local carg = arg
  return function(...) return call(%f, tappend(arg, %carg)) end
end
