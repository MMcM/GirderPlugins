-- Interval (in ms.) between checks.
UnreadMailInterval = 300000

-- Function to use to update.  
-- Some possibilities are:
--   UnreadMailFromRegistry
--   function() local t,f = UnreadMailFromRegistry(); return f end
--   UnreadMailFromOutlook
--   function() return UnreadMailFromOutlook(1,1) end
UnreadMailFunction = UnreadMailFromRegistry

-- Called on Idle to periodically call the check function.
function UnreadMailUpdate()
  local now = GetTickCount()
  if not UnreadMailLastTime or (now - UnreadMailLastTime) &gt;= UnreadMailInterval then
    UnreadMail = UnreadMailFunction()
    UnreadMailLastTime = now
  end
end

-- Get the information used by SHGetUnreadMailCount, both as a total
-- and broken down by mailbox.
UnreadMailRoot = "Software\\Microsoft\\Windows\\CurrentVersion\\UnreadMail"
function UnreadMailFromRegistry()
  local total, formatted = 0, nil

  local ro, error, keys, unread
  ro = REGISTRY_CreateObject()
  local ret = ro.OpenKey("HKCU", UnreadMailRoot)
  if ret then return end
  
  keys, error = ro.ListKeys()
  if error ~= 0 then return end
  
  for i = 1, getn(keys) do
    ret = ro.OpenKey("HKCU", UnreadMailRoot .. "\\" .. keys[i])
    if not ret then
      unread, error = ro.Read("MessageCount")
      if error == 0 then
        total = total + unread
        if formatted then
          formatted = formatted .. "+" .. unread
        else
          formatted = unread
        end
      end
    end
  end
  
  ro.CloseKey()
  return total, formatted
end

-- GetObject or nil
function ProtectedGetObject(...)
  return call(luacom.GetObject, arg, "x", nil)
end

-- items(index) or nil
function ProtectedGetItem(...)
  return call(arg[1].Item, arg, "x", nil)
end

-- Get unread mail count from outlook, optionally including amount of
-- spam removed and counts from other stores with folders named
-- "Inbox".  This is useful when an inbox rule moves incoming mail
-- from some account(s) to another store.  Outlook will no longer
-- report that as unread to the shell.
function UnreadMailFromOutlook(includeJunk, includeOtherStores)
  local outlook, session, defaultInbox, enum, store, otherBox, unread, result

  if not luacom then
    LoadLuaCom()
  end

  -- Obtain connection each time, in case application has been closed
  -- and reopened.
  outlook = ProtectedGetObject("Outlook.Application")
  if not outlook then return end

  session = outlook.Session
  defaultInbox = session:GetDefaultFolder(6) -- olFolderInbox

  unread = defaultInbox.UnReadItemCount
  result = unread

  if includeJunk then
    otherBox = session:getDefaultFolder(23) -- olFolderJunk
    unread = otherBox.UnReadItemCount
    if unread &gt; 0 then
      result = result .. "(-" .. unread .. ")"
    end
  end

  if includeOtherStores then
    enum = luacom.GetEnumerator(session.Folders)
    store = enum:Next()
    while store do
      if store.StoreID ~= defaultInbox.StoreID then
        otherBox = ProtectedGetItem(store.Folders, defaultInbox.Name)
        if otherBox then
          unread = otherBox.UnReadItemCount
          result = result .. "+" .. unread
        end
      end
      store = enum:Next()
    end
  end
  
  return result
end
