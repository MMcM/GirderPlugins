          <sValue1>function split(str)
  local result = {}
  local idx = 1, nidx1, nidx2
  repeat
    nidx1, nidx2 = strfind(str, "%s+", idx)
    if nidx1 ~= 1 then
      tinsert(result, 1, strsub(str, idx, (nidx1 or 0) - 1))
    end
    idx = nidx2 and (nidx2 + 1)
  until not idx
  return result
end

