      <Command Name="Init" Identifier="144" Enabled="TRUE" ErrorBreak="FALSE" ErrorJump="FALSE" ErrorEmail="FALSE" ErrorGirderLog="FALSE" ErrorSystemLog="FALSE">
        <OSD>FALSE</OSD>
        <Topmost>TRUE</Topmost>
        <MatchNum>1</MatchNum>
        <sValue1>require 'unreadmail'

-- Interval (in ms.) between checks.
UnreadMailInterval = 300000

-- Function to use to update.  
-- Some possibilities are:
--   function() return tostring(unreadmail:FromRegistry()) end
--   function() return tostring(unreadmail:FromRegistry(true)) end
--   function() return tostring(unreadmail:FromOutlook()) end
--   function() return tostring(unreadmail:FromOutlook(true,true)) end
UnreadMailFunction = function() return tostring(unreadmail:FromRegistry()) end

-- Called on Idle to periodically call the check function.
function UnreadMailUpdate()
  local now = win.GetTickCount()
  if not UnreadMailLastTime or (now - UnreadMailLastTime) &gt;= UnreadMailInterval then
    UnreadMail = UnreadMailFunction()
    UnreadMailLastTime = now
  end
end
