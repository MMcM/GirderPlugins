              { Free = WMIFormatDiskFreeSpace })

  -- Add just the first TCP interface found.
  WMIAddQuery("WMITCP", "SELECT * FROM Win32_PerfFormattedData_Tcpip_NetworkInterface",
              { Name = "Name", 
                Received = "PacketsReceivedPersec", 
                Sent = "PacketsSentPersec" },
              1)
end

if nil then
  -- Get information about the most active process.
  WMIAddEnum("TopProcess", "Win32_PerfFormattedData_PerfProc_Process",
             WMIMostActiveProcess)
elseif nil then
  -- Include the idle process (which will often win).
  WMIAddEnum("TopProcess", "Win32_PerfFormattedData_PerfProc_Process",
             rcurry(WMIMostActiveProcess,1))
end
